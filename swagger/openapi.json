{
    "openapi": "3.0.0",
    "info": {
      "title": "My FastAPI-like C Framework",
      "version": "1.0.0"
    },
    "paths": {
      "/hello": {
        "get": {
          "summary": "Returns a Hello, World! message",
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "text/plain": {
                  "example": "Hello, World!"
                }
              }
            }
          }
        }
      }
    }
  }
  